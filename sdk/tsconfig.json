{
  "compilerOptions": {
    "module": "ESNext",
    "target": "ES6",
    "declaration": true,
    "outDir": "dist",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true,
    "sourceMap": true,
    "resolveJsonModule": true,
  },
  "include": ["src"],
  "exclude": ["node_modules", "dist"]
}
